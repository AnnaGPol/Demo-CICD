    <rule>
        <name>createOrderDetailDisplayName</name>
        <uuid>eed9d367-a60f-4dbc-97d6-249bab5b0ee7</uuid>
        <description>Display name for the New Order Detail process</description>
        <parentUuid>cf6cf636-cbd9-4793-bf92-60e13263cec1</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>"New Order Detail" &amp; if(ri!cancel, " (Cancelled)", ": " &amp; ri!record.type)</definition>
        <namedTypedValue>
            <name>cancel</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>record</name>
            <type>
                <name>OrderDetail</name>
                <namespace>urn:com:appian:types</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
